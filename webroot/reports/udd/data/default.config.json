{
  "widgetContentViewList": ["WidgetLineChartView"],
  "defaultViewConfigs": {
    "WidgetLineChartView": {
      "contentConfigView": "WidgetChartConfigView",
      "dataConfigViews": ["WidgetQueryConfigView"]
    },
    "WidgetChartConfigView": {
      "view": "WidgetLineChartView",
      "viewPathPrefix": "reports/udd/ui/js/views/",
      "viewConfig": {
        "color": "1f77b4",
        "yAxisLabel": "Query Count",
        "yAxisValue": "COUNT(query_stats)"
      }
    },
    "WidgetQueryConfigView": {
      "view": "WidgetQueryConfigView",
      "viewPathPrefix": "reports/udd/ui/js/views/",
      "viewConfig": {
        "table_name": "StatTable.QueryPerfInfo.query_stats",
        "select": "T=, COUNT(query_stats)",
        "time_range": 3600
      }
    }
  }
}
